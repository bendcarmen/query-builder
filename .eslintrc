{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "max-len": [1, 120, 2, { "ignoreComments": true }],
    "prop-types": [0],
    "import/prefer-default-export": 0,
    "react/jsx-filename-extension": 0,
    "jsx-a11y/click-events-have-key-events": 0,
    "jsx-a11y/interactive-supports-focus": 0
  },
  "extends": ["airbnb"]
}